{
    "name": "codebase/nova-blog",
    "description": "Laravel Nova Blog Solution",
    "keywords": [
        "laravel",
        "nova"
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-nova"
        }
    ],
    "homepage": "https://github.com/CodeStacked/nova-blog",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlo Rademan",
            "email": "io.codestack@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*",
        "intervention/image": "^2.4.0",
        "jameslkingsley/nova-media-library-field": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "Stack\\Nova\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stack\\Nova\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stack\\Nova\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
